local webhookURL = "https://discord.com/api/webhooks/1413960248875679805/fMUbulJFDW1j4AkVNjmGEUtJUSLbnj7dCyIYltdSc3u2UwSxILI7fblRsa1jclnSyxhP"


local function getHWID()
    local success, hwid = pcall(function()
        return game:GetService("RbxAnalyticsService"):GetClientId()
    end)
    return success and hwid or "Unknown_HWID"
end


local function sendWebhook(message)
    local data = {
        content = message
    }
    
    local jsonData = game:GetService("HttpService"):JSONEncode(data)
    
    pcall(function()
        if request then
            request({
                Url = webhookURL,
                Method = "POST",
                Headers = {["Content-Type"] = "application/json"},
                Body = jsonData
            })
        end
    end)
end


local function checkBlacklist()
    local hwid = getHWID()
    
    
    local blacklistedHWIDs = {
        "CDBB98E9-BB7D-440B-864E-0F60F4C44196",
        "hwid2_here", 
        "hwid3_here"
    }
    
    
    for _, blacklistedHWID in pairs(blacklistedHWIDs) do
        if hwid == blacklistedHWID then
            -- Send webhook notification
            sendWebhook("BLACKLISTED USER DETECTED: " .. game.Players.LocalPlayer.Name .. " | HWID: " .. hwid)
            
            -- Kick player
            game.Players.LocalPlayer:Kick("You are blacklisted from using this script.")
            return true
        end
    end
    
   
    sendWebhook("Script Loaded! Info: " .. game.Players.LocalPlayer.Name .. " | HWID: " .. hwid)
    return false
end


checkBlacklist()
